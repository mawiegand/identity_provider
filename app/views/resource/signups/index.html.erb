<h1>Listing resource_signups</h1>

<table>
  <tr>
    <th>Client</th>
    <th>Identity</th>
    <th>Invitation</th>
    <th>Automatic</th>
    <th>Ip</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @resource_signups.each do |resource_signup| %>
  <tr>
    <td><%= resource_signup.client_id %></td>
    <td><%= resource_signup.identity_id %></td>
    <td><%= resource_signup.invitation %></td>
    <td><%= resource_signup.automatic %></td>
    <td><%= resource_signup.ip %></td>
    <td><%= link_to 'Show', resource_signup %></td>
    <td><%= link_to 'Edit', edit_resource_signup_path(resource_signup) %></td>
    <td><%= link_to 'Destroy', resource_signup, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Signup', new_resource_signup_path %>
