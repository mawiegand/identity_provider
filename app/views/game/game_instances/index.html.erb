    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @game_game_instances.each do |game_game_instance| %>
  <tr>
    <td><%= game_game_instance.game_id %></td>
    <td><%= game_game_instance.scope %></td>
    <td><%= game_game_instance.number %></td>
    <td><%= game_game_instance.name %></td>
    <td><%= game_game_instance.localized_name %></td>
    <td><%= game_game_instance.description %></td>
    <td><%= game_game_instance.localized_description %></td>
    <td><%= game_game_instance.available_since %></td>
    <td><%= game_game_instance.started_at %></td>
    <td><%= game_game_instance.ended_at %></td>
    <td><%= game_game_instance.signin_enabled %></td>
    <td><%= game_game_instance.signup_enabled %></td>
    <td><%= game_game_instance.insider_only %></td>
    <td><%= game_game_instance.testing %></td>
    <td><%= game_game_instance.speed_factor %></td>
    <td><%= game_game_instance.estimated_duration_min %></td>
    <td><%= game_game_instance.estimated_duration_max %></td>
    <td><%= game_game_instance.multi_language %></td>
    <td><%= game_game_instance.language_codes %></td>
    <td><%= game_game_instance.max_players %></td>
    <td><%= game_game_instance.present_players %></td>
    <td><%= game_game_instance.hidden %></td>
    <td><%= game_game_instance.hidden_for_non_insiders %></td>
    <td><%= game_game_instance.restriction_country_codes %></td>
    <td><%= game_game_instance.restriction_language_codes %></td>
    <td><%= game_game_instance.restriction_latitude %></td>
    <td><%= game_game_instance.restriction_longitude %></td>
    <td><%= game_game_instance.restriction_latitude_max %></td>
    <td><%= game_game_instance.restriction_longitude_max %></td>
    <td><%= game_game_instance.restriction_distance_to_position %></td>
    <td><%= game_game_instance.restriction_postal_code %></td>
    <td><%= game_game_instance.server_types %></td>
    <td><%= link_to 'Show', game_game_instance %></td>
    <td><%= link_to 'Edit', edit_game_game_instance_path(game_game_instance) %></td>
    <td><%= link_to 'Destroy', game_game_instance, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

