<h1>Listing game_servers</h1>

<table>
  <tr>
    <th>Game instance</th>
    <th>Type string</th>
    <th>Hostname</th>
    <th>Port</th>
    <th>Protocol</th>
    <th>Namespace</th>
    <th>Ip</th>
    <th>Online</th>
    <th>Available since</th>
    <th>Ended at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @game_servers.each do |game_server| %>
  <tr>
    <td><%= game_server.game_instance_id %></td>
    <td><%= game_server.type_string %></td>
    <td><%= game_server.hostname %></td>
    <td><%= game_server.port %></td>
    <td><%= game_server.protocol %></td>
    <td><%= game_server.namespace %></td>
    <td><%= game_server.ip %></td>
    <td><%= game_server.online %></td>
    <td><%= game_server.available_since %></td>
    <td><%= game_server.ended_at %></td>
    <td><%= link_to 'Show', game_server %></td>
    <td><%= link_to 'Edit', edit_game_server_path(game_server) %></td>
    <td><%= link_to 'Destroy', game_server, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Server', new_game_server_path %>
